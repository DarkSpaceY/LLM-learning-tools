# 生产环境配置
VITE_API_BASE_URL=/api
VITE_ENV=production
VITE_DEFAULT_MODEL=deepseek-r1:8b
VITE_TIMEOUT=60000
VITE_ENABLE_LOGS=false
VITE_MAX_MESSAGES=50
VITE_ENABLE_STREAMING=true
VITE_ENABLE_MARKDOWN=true
VITE_ENABLE_CODE_HIGHLIGHT=true
VITE_ENABLE_LATEX=false
VITE_ENABLE_MERMAID=false
VITE_CHAT_HISTORY=true
VITE_AUTO_SCROLL=true
VITE_ENABLE_SUGGESTIONS=false
VITE_ENABLE_FILE_UPLOAD=false

# 性能优化
VITE_CHUNK_SIZE_WARNING_LIMIT=2000
VITE_BUILD_COMPRESS=true
VITE_BUILD_SOURCEMAP=false
VITE_DROP_CONSOLE=true
VITE_DROP_DEBUGGER=true
VITE_IMAGE_COMPRESS=true

# 缓存设置
VITE_PWA_ENABLE=true
VITE_CACHE_TIME=7200
VITE_MAX_CACHE_SIZE=50
VITE_PERSIST_KEY=ai-education-storage

# 安全设置
VITE_CSP_ENABLE=true
VITE_XSS_PROTECTION=true
VITE_CONTENT_SECURITY_POLICY="default-src 'self'; connect-src 'self' http://localhost:8000;"